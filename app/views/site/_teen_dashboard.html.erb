<%= render partial: "shared/teen_nav" %>
<div class="ma_back">
	<div class="row ma_flashalert">
		<%= flash.notice %>
	</div>
	<div class="row">
		<div class="large-4 large-offset-4 columns heb ma_greet">
			שלום!
		</div>
	</div>
	<div class="row">
		<table class = "large-6 columns">
			<thead>
				<tr>
					<td>Mid-Year</td>
					<td>Time Stamp</td>
				</tr>
			</thead>
			<tbody>
				<% @form_mid.each do |fn| %>
				<tr>
					<td><%= link_to fn.id, teen_mid_year_url(fn.id) %></td>
					<td><%= fn.updated_at %></td>
				</tr>
				<% end %>
			</tbody>
		</table>
		<table class = "large-6 columns">
			<thead>
				<tr>
					<td>End of Year Evaluation and Reapplication</td>
					<td>Time Stamp</td>
				</tr>
			</thead>
			<tbody>
				<% @form_end.each do |fn| %>
				<tr>
					<td><%= link_to fn.id, teen_end_year_url(fn.id) %></td>
					<td><%= fn.updated_at %></td>
				</tr>
				<% end %>
			</tbody>
		</table>
	</div>
	<div class="row">
		<table class = "large-6 columns">
			<thead>
				<tr>
					<td>Application</td>
					<td>Time Stamp</td>
				</tr>
			</thead>
			<tbody>
				<% @form_new.each do |fn| %>
				<tr>
					<td><%= link_to fn.id, teen_new_url(fn.id) %></td>
					<td><%= fn.updated_at %></td>
				</tr>
				<% end %>
			</tbody>
		</table>
		<table class = "large-6 columns">
			<thead>
				<tr>
					<td>Non-Returning Application</td>
					<td>Time Stamp</td>
				</tr>
			</thead>
			<tbody>
				<% @form_term.each do |fn| %>
					<tr>
						<td><%= link_to fn.id, teen_term_url(fn.id) %></td>
						<td><%= fn.updated_at %></td>
					</tr>
					<% end %>
			</tbody>
		</table>
	</div>
	<div class="row">
		<table class = "large-12 columns">
			<thead>
				<tr>
					<td>Incident Reports</td>
					<td>Time Stamp</td>
				</tr>
			</thead>
			<tbody>
				<% @incident.each do |fn| %>
				<tr>
					<td><%= link_to fn.id, incident_url(fn.id) %></td>
					<td><%= fn.updated_at %></td>
				</tr>
				<% end %>
			</tbody>
		</table>
	</div>
</div>