<div class="ma_back">	
	<div class="row">
		<div class="ma_head">
			<h1 class="ma_title">CTJ<br>Madrichim</h1>
		</div>
	</div>
	<div class="row">
		<div class="large-6 columns ma_greet">
			<%= @teen.full_name %>
		</div>
		<div class="large-6 columns">
			<%= link_to "Back to Dashboard", root_url, :class => "button" %>
		</div>
	</div>
	<div class="row">
		<table class = "large-6 columns">
			<thead>
				<tr>
					<td>Mid-Year</td>
					<td>Time Stamp</td>
				</tr>
			</thead>
			<tbody>
				<% @form_mid.each do |fn| %>
				<tr>
					<td><%= link_to fn.id, teen_mid_year_url(fn.id) %></td>
					<td><%= fn.updated_at %></td>
				</tr>
				<% end %>
			</tbody>
		</table>
		<table class = "large-6 columns">
			<thead>
				<tr>
					<td>Teacher Mid-Year</td>
					<td>Teacher Name</td>
					<td>Time Stamp</td>
				</tr>
			</thead>
			<tbody>
				<% @form_teach_mid.each do |fn| %>
				<tr>
					<td><%= link_to fn.id, teacher_mid_year_url(fn.id) %></td>
					<td><%= fn.user.full_name %></td>
					<td><%= fn.updated_at %></td>
				</tr>
				<% end %>
			</tbody>
		</table>
	</div>
	<div class="row">
		<table class = "large-6 columns">
			<thead>
				<tr>
					<td>End of Year Evaluation and Reapplication</td>
					<td>Time Stamp</td>
				</tr>
			</thead>
			<tbody>
				<% @form_end.each do |fn| %>
				<tr>
					<td><%= link_to fn.id, teen_end_year_url(fn.id) %></td>
					<td><%= fn.updated_at %></td>
				</tr>
				<% end %>
			</tbody>
		</table>
		<table class = "large-6 columns">
			<thead>
				<tr>
					<td>End of Year Teacher Evaluation</td>
					<td>Teacher Name</td>
					<td>Time Stamp</td>
				</tr>
			</thead>
			<tbody>
				<% @form_teach_end.each do |fn| %>
				<tr>
					<td><%= link_to fn.id, teacher_end_year_url(fn.id) %></td>
					<td><%= fn.user.full_name %></td>
					<td><%= fn.updated_at %></td>
				</tr>
				<% end %>
			</tbody>
		</table>
	</div>
	<div class="row">
		<table class = "large-6 columns">
			<thead>
				<tr>
					<td>Application</td>
					<td>Time Stamp</td>
				</tr>
			</thead>
			<tbody>
				<% @form_new.each do |fn| %>
				<tr>
					<td> <%= link_to fn.id, teen_new_url(fn.id) %> </td>
					<td> <%= fn.updated_at %> </td>
				</tr>
				<% end %>
			</tbody>
		</table>
		<table class = "large-6 columns">
			<thead>
				<tr>
					<td>Non-Returning Application</td>
					<td>Time Stamp</td>
				</tr>
			</thead>
			<tbody>
				<% @form_term.each do |fn| %>
				<tr>
					<td><%= link_to fn.id, teen_term_url(fn.id) %></td>
					<td><%= fn.updated_at %></td>
				</tr>
				<% end %>
			</tbody>
		</table>
	</div>
	<div class = "row">
		<table class = "large-6 columns">
			<thead>
				<tr>
					<td>Incidents Involved In</td>
					<td>Reported By</td>
					<td>Time Stamp</td>
				</tr>
			</thead>
			<tbody>
				<% @incident.each do |fn| %>
				<tr>
					<td><%= link_to fn.id, incident_url(fn.id) %></td>
					<td><%= fn.user.full_name %></td>
					<td><%= fn.updated_at %></td>
				</tr>
				<% end %>
			</tbody>
		</table>
		<table class = "large-6 columns">
			<thead>
				<tr>
					<td>Incidents Reported</td>
					<td>Time Stamp</td>
				</tr>
			</thead>
			<tbody>
				<% @incident_r.each do |fn| %>
				<tr>
					<td><%= link_to fn.id, incident_url(fn.id) %></td>
					<td><%= fn.updated_at %></td>
				</tr>
				<% end %>
			</tbody>
		</table>
	</div>
	<div class = "row">
		<table class = "large-6 columns">
			<thead>
				<tr>
					<td>Interview Forms</td>
					<td>Time Stamp</td>
				</tr>
			</thead>
			<tbody>
				<% @interview.each do |fn| %>
				<tr>
					<td><%= link_to fn.id, admin_interview_url(fn.id) %></td>
					<td><%= fn.updated_at %></td>
				</tr>
				<% end %>
			</tbody>
		</table>
</div>