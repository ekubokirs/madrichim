<div class="ma_back">	
	<div class="row">
		<div class="ma_head">
			<h1 class="ma_title">CTJ<br>Madrichim</h1>
		</div>
	</div>
	<div class="ma_greet">
		<%= @teen.full_name %>
	</div>
	<div class="row">
		<table class = "large-6 columns">
			<thead>
				<tr>
					<td>Mid-Year</td>
					<td>Time Stamp</td>
				</tr>
			</thead>
			<tbody>
				<% @form_mid.each do |fn| %>
				<tr>
					<td> <%= link_to fn.id, teen_mid_year_url(@teen) %> </td>
					<td> <%= fn.updated_at %> </td>
				</tr>
				<% end %>
			</tbody>
		</table>
		<table class = "large-6 columns">
			<thead>
				<tr>
					<td>Teacher Mid-Year</td>
					<td>Time Stamp</td>
				</tr>
			</thead>
			<tbody>
				<% @form_teach_mid.each do |fn| %>
				<tr>
					<td> <%= link_to fn.id %> </td>
					<td> <%= fn.updated_at %> </td>
				</tr>
				<% end %>
			</tbody>
		</table>
	</div>
	<div class="row">
		<table class = "large-6 columns">
			<thead>
				<tr>
					<td>End of Year Evaluation and Reapplication</td>
					<td>Time Stamp</td>
				</tr>
			</thead>
			<tbody>
				<% @form_end.each do |fn| %>
				<tr>
					<td> <%= link_to fn.id %> </td>
					<td> <%= fn.updated_at %> </td>
				</tr>
				<% end %>
			</tbody>
		</table>
		<table class = "large-6 columns">
			<thead>
				<tr>
					<td>End of Year Teacher Evaluation</td>
					<td>Time Stamp</td>
				</tr>
			</thead>
			<tbody>
				<% @form_teach_end.each do |fn| %>
				<tr>
					<td> <%= link_to fn.id %> </td>
					<td> <%= fn.updated_at %> </td>
				</tr>
				<% end %>
			</tbody>
		</table>
	</div>
	<div class="row">
		<table class = "large-6 columns">
			<thead>
				<tr>
					<td>Application</td>
					<td>Time Stamp</td>
				</tr>
			</thead>
			<tbody>
				<% @form_new.each do |fn| %>
				<tr>
					<td> <%= link_to fn.id %> </td>
					<td> <%= fn.updated_at %> </td>
				</tr>
				<% end %>
			</tbody>
		</table>
		<table class = "large-6 columns">
			<thead>
				<tr>
					<td>Non-Returning Application</td>
					<td>Time Stamp</td>
				</tr>
			</thead>
			<tbody>
				<%# @form_mid.each do |fn| %>
				<tr>
					<td> <%#= link_to fn.id %> </td>
					<td> <%#= fn.updated_at %> </td>
				</tr>
				<%# end %>
			</tbody>
		</table>
	</div>
</div>