<%= render partial: "shared/admin_nav" %>
<div class="ma_back">	
	<div class="row ma_flashalert">
		<%= flash.notice %>
	</div>
	<div class="row">
		<% @teachMY.each do |fn| %>
			<ul>
				<li>
					<%= link_to fn.id, teacher_mid_year_url(fn.id) %>
					<ul>
						<li><%= fn.user.full_name %></li>
						<li><%= fn.teen.full_name %></li>
						<li><%= fn.updated_at.strftime("%m/%d/%Y") %></li>
						<li><%= link_to "Edit", edit_teacher_mid_year_url(fn.id) %></li>
						<li><%= link_to "Delete", teacher_mid_year_url(fn.id), method: :delete %></li>
					</ul>
				</li>
			</ul>
		<% end %>
	</div>
</div>