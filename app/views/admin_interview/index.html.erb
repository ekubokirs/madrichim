<%= render partial: "shared/admin_nav" %>
<div class="ma_back">	
	<div class="row ma_flashalert">
		<%= flash.notice %>
	</div>
	<table class="row">
		<thead>
			<tr>
				<td>Interview Comments</td>
				<td>Interviewer Name</td>
				<td>Teen Name</td>
				<td>Time Stamp</td>
				<td>Edit</td>
				<td>Delete</td>
			</tr>
		</thead>
		<tbody>
			<% @interview.each do |fn| %>
			<tr>
				<td><%= link_to fn.id, admin_interview_url(fn.id) %></td>
				<td><%= fn.admin.full_name %></td>
				<td><%= fn.teen.full_name %></td>
				<td><%= fn.updated_at %></td>
				<td><%= link_to "Edit", edit_admin_interview_url(fn.id) %></td>
				<td><%= link_to "Delete", admin_interview_url(fn.id), method: :delete %></td>
			</tr>
			<% end %>
		</tbody>
	</table>
</div>